package JavaClass;

public class House {

	String houseName = "Hasib's Palace";

	int bedRoom = 6;
	double bathRoom = 2.5;

	public void livingRoom() {

		System.out.println("I can Play Soccer in my living room");

	}

	public int kitchen(int window) {

		System.out.println("There are " + window + " window in the kitchen");

		return window;
	}

}
